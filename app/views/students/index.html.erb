    <div class="row">
      <div class="col s12"><h3 class="center-align">Student List</h3></div>
    </div>
    <div class="row">
    <% @students.all.each do |student| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= student.name %></span>
          <p><%= student.email %></p>
        </div>
        <div class="card-action">
          <%= link_to "View Profile", student_path(student) %>
        </div>
      </div>
    </div>
    <% end %>
  </div>