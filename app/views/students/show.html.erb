 <div class="row">
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= @student.name %></span>
          <p><%= @student.email %></p>
          <% if @student.courses.empty? %>
          <p>None</p>
           <% else %>
           <ul>
           <% @student.courses.each do |course|%>
           <li><%= course.name %>
           <% end %>
           </ul>

           <% end %>
        </div>
      </div>
      <% if current_user == @student %>
      <%= link_to '<i class="material-icons right">create</i>Edit Profile'.html_safe, edit_student_path(@student), class: "waves-effet waves-light btn" %>
      <% end %>
    </div>
  </div>